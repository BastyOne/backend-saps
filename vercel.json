{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_KEY": "@supabase-key",
    "JWT_SECRET": "@jwt-secret",
    "EMAIL_USER": "@email-user",
    "EMAIL_PASS": "@email-pass"
  }
}
