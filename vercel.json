{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/alumnos",
        "dest": "/routes/alumnosRoutes.js"
      },
      {
        "src": "/alumnos/(.*)",
        "dest": "/routes/alumnosRoutes.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "SUPABASE_URL": "@supabase_url",
      "SUPABASE_KEY": "@supabase_key"
    }
  }
  